<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<VBox fx:id="root" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.ks.idnadrev.thought.view.ViewThoughtsController">
    <children>
        <StackPane fx:id="crudContainer"/>
        <SplitPane fx:id="split" dividerPositions="0.36" VBox.vgrow="ALWAYS">
            <items>
                <StackPane fx:id="tableContainer" prefHeight="150.0" prefWidth="200.0"/>
                <StackPane fx:id="previewContainer" prefHeight="150.0" prefWidth="200.0"/>
            </items>
        </SplitPane>
    </children>
</VBox>

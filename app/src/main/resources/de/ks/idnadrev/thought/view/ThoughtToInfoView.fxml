<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.FlowPane?>
<FlowPane hgap="10.0" vgap="10.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="de.ks.idnadrev.thought.view.ThoughtToInfoController">
    <children>
        <Button fx:id="toTextBtn" mnemonicParsing="false" onAction="#toText" text="%thought.transform.text"/>
        <Button fx:id="toLinkBtn" mnemonicParsing="false" onAction="#toLink" text="%thought.transform.hyperlink"/>
        <Button fx:id="toFileBtn" mnemonicParsing="false" onAction="#toFile" text="%thought.transform.file"/>
    </children>
</FlowPane>

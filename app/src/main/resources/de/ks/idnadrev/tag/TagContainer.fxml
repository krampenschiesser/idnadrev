<?import javafx.scene.layout.*?>
<GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
          prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
          fx:controller="de.ks.idnadrev.tag.TagContainer">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
    </rowConstraints>
    <children>
        <StackPane fx:id="tagSelectionContainer" prefHeight="150.0" prefWidth="200.0">
            <children>
                <fx:include fx:id="tagAdd" source="../selection/NamedPersistentObjectSelection.fxml"/>
            </children>
        </StackPane>
        <FlowPane fx:id="tagPane" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1"/>
    </children>
</GridPane>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.StackPane?>
<TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity"
         tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="de.ks.idnadrev.task.create.CreateTask">
    <tabs>
        <Tab text="%task.create.main.info">
            <content>
                <StackPane prefHeight="150.0" prefWidth="200.0">
                    <children>
                        <fx:include fx:id="mainInfo" source="MainTaskInfo.fxml"/>
                    </children>
                </StackPane>
            </content>
        </Tab>
        <Tab text="%task.create.files">
            <content>
                <StackPane prefHeight="150.0" prefWidth="200.0">
                    <children>
                        <fx:include fx:id="fileView" source="../../../file/FileView.fxml"/>
                    </children>
                </StackPane>
            </content>
        </Tab>
        <Tab text="%task.create.schedule.info">
            <content>
                <StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"/>
            </content>
        </Tab>
        <Tab text="%task.outcome.expected">
            <content>
                <StackPane>
                    <children>
                        <fx:include fx:id="expectedOutcome" source="TaskOutcome.fxml"/>
                    </children>
                </StackPane>
            </content>
        </Tab>
    </tabs>
</TabPane>

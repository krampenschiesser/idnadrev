<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="de.ks.gallery.ui.thumbnail.ThumbnailGallery">
    <children>
        <FlowPane fx:id="container" hgap="10.0" vgap="10.0">
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </padding>
        </FlowPane>
        <ProgressIndicator fx:id="loader">
            <StackPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </StackPane.margin>
        </ProgressIndicator>
    </children>
</StackPane>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<StackPane id="StackPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
           prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2"
           fx:controller="de.ks.activity.ActivityHome">
    <children>
        <GridPane>
            <children>
                <Button fx:id="showDetails" mnemonicParsing="false" text="Show Details" GridPane.columnIndex="0"
                        GridPane.rowIndex="3"/>
                <Button fx:id="switchView" mnemonicParsing="false" text="Switch View" GridPane.columnIndex="1"
                        GridPane.rowIndex="3"/>
                <TextField fx:id="id" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
                <TextField fx:id="name" prefWidth="200.0" GridPane.columnIndex="1" GridPane.rowIndex="1"/>
                <Label text="ID:" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
                <Label text="Name:" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
            </children>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="NEVER"/>
                <RowConstraints maxHeight="1.7976931348623157E308" minHeight="10.0" prefHeight="75.0" vgrow="ALWAYS"/>
                <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="75.0" valignment="BOTTOM"
                                vgrow="NEVER"/>
            </rowConstraints>
            <StackPane.margin>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
            </StackPane.margin>
        </GridPane>
    </children>
</StackPane>
